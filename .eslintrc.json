{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier", "svelte3"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "overrides": [{
    "files": ["**/*.svelte"],
    "processor": "svelte3/svelte3"
  }]
}